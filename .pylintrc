[MASTER]
load-plugins=pylint_common, pylint_django
ignore=config.settings

[FORMAT]
max-line-length=120

[MESSAGES CONTROL]
disable=missing-docstring,invalid-name,too-few-public-methods

[DESIGN]
max-parents=15

[TYPECHECK]
generated-members=REQUEST,acl_users,aq_parent,"[a-zA-Z]+_set{1,2}",save,delete
